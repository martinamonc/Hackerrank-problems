def aVeryBigSum(ar):
    sums= 0
    for a in ar:
        sums+=a
    return sums

TIME CONVERSION
def timeConversion(s):
    hh, mm, ss= s.split(':')
    if 'AM' in ss:
        if hh=='12':
            hh1= '00'
        else:
            hh1==hh
        ss= ss.replace('AM', '')
    elif 'PM' in ss:
        if hh=='12':
            hh1= '12'
        else:
            hh1= int(hh)+12
        ss= ss.replace('PM', '')
    time= [str(hh1), mm, ss]
    return ':'.join(time)

