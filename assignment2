RUNNING TIME OF ALGORITHMS
def runningTime(arr):
    z=0
    if arr== (sorted(arr)):
        return (0)    
    else:
        for i in range(1,len(arr)):
            x=arr[i]
            for j in range(i):
                if arr[j]>x:
                    (arr[i],arr[j])=(arr[j],arr[i])
                    z+=1
        return z

CLOSEST NUMBERS
def closestNumbers(arr):
    count= {}
    couples= []
    for i in range(len(arr)):
        succ= i+1 
        0<succ<=len(arr)
        sarr= sorted(arr)
        a1, a2= sarr[i], sarr[succ]
        d= a2-a1
        k= tuple([a1,a2])
        count[k]= d
    return count.keys()

